{
    "name": "mewesk/twig-excel-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony2 bundle provides a PhpExcel integration for Twig",
    "keywords": ["excel","phpexcel","twig","symfony","symfony2"],
    "homepage": "https://github.com/MewesK/TwigExcelBundle",
    "license": "MIT",
    "authors": [
        { "name": "Mewes Kochheim", "email": "mewes@kochheim.de" }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.0",
        "symfony/symfony": "~2.0",
        "phpoffice/phpexcel": "1.8.1"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "~2.0",
        "phpunit/phpunit": "~4"
    },
    "suggest": {
        "mpdf/mpdf": "6.0.0"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigExcelBundle\\": "" }
    },
    "target-dir": "MewesK/TwigExcelBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
